# Browser Automation Environment Variables
# Copy this file to .env and fill in your values

# Session Security
SESSION_SECRET=your-secure-session-secret-key-here

# Default Login Credentials
LOGIN_USERNAME=your-username-or-email
LOGIN_PASSWORD=your-password

# Browser Configuration
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Safari/537.36
PROXY=

# Database (optional, for future enhancements)
DATABASE_URL=sqlite:///browser_automation.db

# Logging Level
LOG_LEVEL=DEBUG
